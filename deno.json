{
  "name": "@dreamer/auth",
  "version": "1.0.0-beta.1",
  "exports": {
    ".": "./src/mod.ts",
    "./jwt": "./src/jwt.ts",
    "./session": "./src/session.ts",
    "./oauth": "./src/oauth.ts",
    "./refresh": "./src/refresh.ts"
  },
  "imports": {
    "@dreamer/test": "jsr:@dreamer/test@^1.0.0-beta.40",
    "@dreamer/crypto": "jsr:@dreamer/crypto@^1.0.0-beta.2",
    "@dreamer/session": "jsr:@dreamer/session@1.0.0-beta.3"
  },
  "tasks": {
    "test": "deno test -A tests/",
    "check": "deno check src/mod.ts",
    "lint": "deno lint src/ tests/",
    "fmt": "deno fmt src/ tests/"
  },
  "publish": {
    "include": [
      "src/**/*.ts",
      "deno.json",
      "README.md",
      "LICENSE.md"
    ],
    "exclude": [
      "tests/**",
      "**/*.test.ts"
    ]
  },
  "nodeModulesDir": "auto",
  "compilerOptions": {
    "strict": true,
    "lib": ["esnext", "dom", "dom.iterable", "dom.asynciterable", "deno.ns", "deno.window"]
  }
}
